@using IdeoTreeStructure.Domain.Entities
@model IEnumerable<TreeNode>
@{
    ViewBag.Title = "Index";
}

<div class="p-3">
    @foreach (var n in Model)
    {
        <div class="bg-light border border-primary rounded m-2">
            <h3>@n.NodeID</h3>
            @n.Content
        </div>
    }
</div>